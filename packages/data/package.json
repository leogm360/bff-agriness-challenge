{
  "name": "@agriness/data",
  "version": "0.0.1",
  "description": "Camada de dados do desafio agriness",
  "author": "Leonardo Moraes",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./prisma": {
      "import": "./dist/prisma/index.js",
      "require": "./dist/prisma/index.js",
      "types": "./dist/prisma/index.d.ts"
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "prebuild": "prisma generate",
    "build": "tsup",
    "start": "tsup && prisma migrate deploy && prisma migrate deploy",
    "dev": "tsup --watch",
    "prisma:generate": "prisma generate",
    "prisma:migrate:create": "prisma migrate dev --create-only"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.3.9",
    "reflect-metadata": "^0.2.0"
  },
  "devDependencies": {
    "@agriness/typescript": "*",
    "@swc/core": "^1.6.6",
    "glob": "^10.4.2",
    "prisma": "^5.16.1",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2"
  }
}
